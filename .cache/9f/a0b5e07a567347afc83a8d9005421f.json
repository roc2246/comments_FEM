ilities/functions\" as *;\n\n.vote{\n    &__btn{\n        width: rem(40px);\n        height: rem(40px);\n    }\n}\n\n.CRUD-container{\n    width: max-content;\n}","scss\\components\\_ruler.scss":"@use \"utilities/functions\" as *;\n@use \"utilities/variables\" as *;\n\n.reply-wrapper__ruler {\n  @media (min-width: $ruler-break) {\n  margin-left: auto;\n  }\n}\n","scss\\layouts\\_main-sections.scss":"@use \"utilities/functions\" as *;\n\nmain{\n    margin:clamp(rem(30px), vw(30px), rem(60px)) auto;\n    width: clamp(rem(345px), vw(345px), rem(730px)); \n}","scss\\layouts\\_comments.scss":"@use \"utilities/functions\" as *;\n\n.comment {\n  padding: clamp(rem(16px), vw(16px), rem(25px));\n  border-radius: 0.5rem;\n}\n","scss\\layouts\\_forms.scss":".vote{\n    width: max-content;\n    border-radius: .75rem;\n    @media (min-width: 600px) {\n        margin-right: 1rem;\n      }\n}\n\n.new-comment{\n    &--reply{\n        display: none;\n    }\n    &--update{\n        display: none;\n    }\n}","scss\\layouts\\_CRUD.scss":".CRUD-container{\n    margin-left: auto;\n}","scss\\layouts\\_flex.scss":"@use \"utilities/functions\" as *;\n\n.vote{\n    display: flex;\n    flex-direction: row;\n    flex-wrap: wrap;\n    align-items: center;\n    justify-content: center;\n    &__btn{\n        display: flex;\n        align-items: center;\n        justify-content: center;\n    }\n    @media (min-width: 600px) {\n        flex-direction: column;\n      }\n}\n\nmain{\n    display: flex;\n    flex-direction: column;\n    gap: clamp(rem(16px), vw(16px), rem(24px));\n\n}\n\n#comment-wrapper{\n    display: flex;\n    flex-direction: column;\n    gap: clamp(rem(16px), vw(16px), rem(24px));\n}\n\n.username {\n    display: flex;\n    flex-direction: row;\n    gap: .25rem;\n}\n\n.CRUD-container{\n    display: flex;\n    flex-direction: row;\n    gap: .75rem;\n}\n\n.CRUD{\n    display: flex;\n    gap: .25rem;\n}","scss\\layouts\\_grid.scss":"@use \"utilities/functions\" as *;\n@use \"utilities/variables\" as *;\n\n.comment {\n  display: grid;\n  grid-template-columns: rem(32px) rem(125px) repeat(5, auto);\n  row-gap: rem(20px);\n  @media (min-width: 600px) {\n    grid-template-columns: auto rem(32px) rem(125px) repeat(4, auto);\n    grid-template-rows: rem(32px) rem(67px) auto;\n    row-gap: unset;\n  }\n  align-items: center;\n\n  .avatar--comment {\n    grid-column: 1/2;\n    grid-row: 1/2;\n    @media (min-width: 600px) {\n      grid-column: 2/3;\n    }\n  }\n  .username {\n    grid-column: 2/3;\n    grid-row: 1/2;\n    @media (min-width: 600px) {\n      grid-column: 3/4;\n    }\n  }\n\n  &__createdAt {\n    grid-column: 3/6;\n    grid-row: 1/2;\n    @media (min-width: 600px) {\n      grid-column: 4/5;\n    }\n  }\n  &__content {\n    grid-column: 1/6;\n    grid-row: 2/3;\n    @media (min-width: 600px) {\n      grid-column: 2/6;\n      grid-row: 2/4;\n    }\n  }\n\n  .vote {\n    grid-column: 1/3;\n    grid-row: 3/4;\n    @media (min-width: 600px) {\n      grid-column: 1/2;\n      grid-row: 1/3;\n    }\n  }\n\n  .CRUD-container {\n    grid-column: 4/6;\n    grid-row: 3/4;\n\n    @media (min-width: 600px) {\n      grid-row: 1/2;\n    }\n  }\n}\n\n$ruler-column-size: clamp(rem(16px), vw(16px), rem(42px));\n\n.reply-wrapper {\n  display: grid;\n  row-gap: clamp(rem(16px), vw(16px), rem(24px));\n  column-gap: $ruler-column-size;\n  grid-template-rows: repeat(2, auto);\n  @media (min-width: $ruler-break) {\n    grid-template-columns: $ruler-column-size auto;\n  }\n\n  &__ruler {\n    grid-column: 1/2;\n    grid-row: 1/ -1;\n  }\n  .comment--reply {\n    grid-column: 2/3;\n  }\n}\n","scss\\themes\\_default.scss":"body{\n    background-color: var(--color-very-light-gray);\n}\n\n.vote{\n    background-color: var(--color-very-light-gray);\n}\n\n.new-comment{\n    background-color: var(--color-white);\n}\n\n.comment{\n    background-color: var(--color-white);\n}\n\n"},"lineCount":null}},"error":null,"hash":"33db4a2946d2d65ab3679ff04d2b2a95","cacheData":{"env":{}}}